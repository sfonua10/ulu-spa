<svg width="1200" height="800" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="corporateBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#E0E8E0;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#D0DCD0;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="corporateAccent" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#8B886E;stop-opacity:0.3" />
      <stop offset="50%" style="stop-color:#A39C7E;stop-opacity:0.5" />
      <stop offset="100%" style="stop-color:#8B886E;stop-opacity:0.3" />
    </linearGradient>
  </defs>

  <!-- Background -->
  <rect width="1200" height="800" fill="url(#corporateBg)"/>

  <!-- Decorative geometric elements -->
  <rect x="100" y="100" width="120" height="120" rx="10" fill="url(#corporateAccent)" opacity="0.2"/>
  <rect x="950" y="550" width="150" height="150" rx="10" fill="url(#corporateAccent)" opacity="0.15"/>
  <circle cx="300" cy="650" r="60" fill="url(#corporateAccent)" opacity="0.2"/>

  <!-- Icon representation - team/corporate -->
  <g transform="translate(600, 350)">
    <circle cx="0" cy="0" r="70" fill="none" stroke="#8B886E" stroke-width="3" opacity="0.6"/>

    <!-- Grid of people -->
    <circle cx="-35" cy="-20" r="12" fill="#8B886E" opacity="0.7"/>
    <circle cx="0" cy="-20" r="12" fill="#8B886E" opacity="0.7"/>
    <circle cx="35" cy="-20" r="12" fill="#8B886E" opacity="0.7"/>

    <circle cx="-35" cy="10" r="12" fill="#8B886E" opacity="0.6"/>
    <circle cx="0" cy="10" r="12" fill="#8B886E" opacity="0.6"/>
    <circle cx="35" cy="10" r="12" fill="#8B886E" opacity="0.6"/>
  </g>

  <!-- Placeholder text -->
  <text x="600" y="550" font-family="sans-serif" font-size="32" fill="#8B886E" text-anchor="middle" opacity="0.6">
    Corporate Wellness
  </text>
  <text x="600" y="590" font-family="sans-serif" font-size="18" fill="#8B886E" text-anchor="middle" opacity="0.5">
    Replace with lifestyle photography
  </text>
</svg>
